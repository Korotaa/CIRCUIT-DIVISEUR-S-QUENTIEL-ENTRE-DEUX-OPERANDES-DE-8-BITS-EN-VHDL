LIBRARY IEEE;
    USE IEEE.STD_LOGIC_1164.all;
    
--************************************************************
-- ENTITY Declaration
--************************************************************

ENTITY MUX_2_1 IS PORT(
A: IN std_logic_vector(7 DOWNTO 0);
B: IN std_logic_vector(7 DOWNTO 0);
sel: IN std_logic;
S: OUT std_logic_vector(7 DOWNTO 0));
END MUX_2_1;

--***********************************************************
-- RTL Description
--***********************************************************

ARCHITECTURE RTL OF MUX_2_1 IS 
BEGIN
    S<=A WHEN sel='1'ELSE B;